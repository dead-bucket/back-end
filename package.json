{
  "name": "dead-bucket-back-end",
  "version": "1.0.0",
  "description": "server and mongo models for backend",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": "8.12.0"
  },
  "scripts": {
    "test": "jest -i",
    "dev": "nodemon",
    "start": "node index.js",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "./__test__/jest-setup.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/__test__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dead-bucket/back-end.git"
  },
  "author": "RND & DM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dead-bucket/back-end/issues"
  },
  "homepage": "https://github.com/dead-bucket/back-end#readme",
  "dependencies": {
    "aws-sdk": "^2.355.0",
    "bcrypt": "^5.0.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "jest": "^25.0.0",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.9",
    "mongoose": "^5.2.8",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.7",
    "superagent": "^4.0.0",
    "uuid": "^3.3.3"
  }
}
